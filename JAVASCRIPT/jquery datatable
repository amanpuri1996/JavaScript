			
      ///sorting date
      <script
			src="//cdn.datatables.net/plug-ins/1.10.11/sorting/date-eu.js"
			type="text/javascript"
		></script>
      
      // add data-order in table
      		<table
						class="display"
						cellspacing="0"
						width="100%"
						data-order='[[ 8, "asc" ]]'
					>
          
          $('.display').DataTable({
				responsive: true,
				order: [[8, 'desc']], //or asc
				columnDefs: [{ targets: 8, type: 'date-eu' }],
			});
